@{
    Layout = null;
}
<!doctype html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html" charset="utf-8" />
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="@ViewBag.V4FileServer/css/common.css" />
    @RenderSection("css", false)
</head>
<body>
    @RenderBody()
    <script src="@ViewBag.V4FileServer/js/jquery-3.4.0.min.js"></script>
    <script src="@ViewBag.V4FileServer/js/layout-common.js"></script>
    <script src="@ViewBag.V4FileServer/js/prompt.js"></script>
    <script src="@ViewBag.V4FileServer/js/loading.js"></script>
    @RenderSection("js", false)
</body>
</html>